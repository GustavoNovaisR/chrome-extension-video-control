{
  "manifest_version": 3,
  "version": "1",
  "name": "Video Controll",
  "description": "Controle para os videos",
  "author": "Gustavo Novais Rodrigues",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "host_permissions":["file://*/*","*://*/*"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
